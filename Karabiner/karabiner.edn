{ 
; -- PROFILES & THRESHOLDS --
        :profiles {
                :Default {
                        :default true

                        :sim     30     ; simultaneous key press threshold (default: 50)
                                        ; simlayer is implemented with to_if_alone and simultaneous key press feature
                                        ; if keydown event for two different keys are pressed within :sim ms, the keypresses are considered simultaneous  
                                        ; this seems to be for [:a :b] kind of cases, and not for the :simlayers (which I don't currently use)
                        :delay   120    ; to_delayed_action_delay_milliseconds (default: 500)
                                        ; basically it means time after which the key press is count delayed
                                        ; after :delay ms, the key press is considered to be delayed (used for double-press macros)
                                        ; NOTE: this should be equal to :held for mod-tap layers
                        :held    120    ; to_if_held_down_threshold_milliseconds (default: 500)
                                        ; key is fired twice when :held ms is elapsed (otherwise seen as a hold command) (NOTE: NO CLUE WHAT IS THIS)
                                        ; NOTE: this should be equal to :delay for mod-tap layers
                        :alone   119    ; to_if_alone_timeout_milliseconds (default: 1000)
                                        ; affects to_if_alone behavior
                                        ; simlayer is implemented with to_if_alone and simultaneous key press feature
                                        ; if keyup event comes within :alone ms from keydown, the key is not just pressed but held
                                        ; NOTE: (I think) this must be just less than the delay / held value to avoid bug where mod-tap layer stays 1
                }
        }

        :simlayer-threshold 30  ;simultaneous_threshold_milliseconds (default: 50) <- this seems to work for the :simlayers cases

; -- TEMPLATES & APPS --
        ; ðŸ‘‡ï¸ This Will make it easy to run & reuse shell commands
        :templates { 
                :alfred "osascript -e 'tell application id \"com.runningwithcrayons.Alfred\" to run trigger \"%s\" in workflow \"%s\" with argument \"%s\"'"
                :btt "osascript -e 'tell application \"BetterTouchTool\"' -e 'trigger_named \"%s\"' -e 'end tell'"
                :km "osascript -e 'tell application \"Keyboard Maestro Engine\" to do script \"%s\" with parameter \"%s\"'"
                :notification "osascript -e 'display notification \"%s\" with title \"%s\"'"
                :open "open \"%s\""
                :open-g "open -g \"%s\""
                :sc "echo \"%s\"|shortcuts run \"%s\" -i -" ; Pay attention - that the Shortcut input goes before the Shortcut name.
                :yabai "/opt/homebrew/bin/yabai -m %s"
        }

        ; ðŸ‘‡ï¸ Useful to later set as conditions
        :applications { 
                :arc ["company.thebrowser.Browser"]
                :bear ["net.shinyfrog.bear"]
                :finalcutpro ["com.apple.FinalCut"]
                :terminal ["com.apple.Terminal"]
                :vscode ["com.microsoft.VSCode"]
                :captureone ["com.captureone.captureone16" :condi [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]]
                :bananotate ["com.amantinband.bananotate"]
                :safari ["com.apple.Safari"]
        }
        
        ; ðŸ‘‡ï¸ This way of creating layers is simple (which is a pro), it allows :condi and :alone, but it doesn't seem to allow the use of :modi (which is a neg). In simlayers it seems to work
        :layers { 
                :rcmd {:key :right_command :condi [:!ropt :!f19-mode]
                        :afterup [{:set ["rcmd" 0]}{:set ["a-rcmd" 0]}{:set ["b-rcmd" 0]}{:set ["c-rcmd" 0]}
                                {:set ["d-rcmd" 0]}{:set ["e-rcmd" 0]}{:set ["f-rcmd" 0]}{:set ["g-rcmd" 0]}
                                {:set ["h-rcmd" 0]}{:set ["i-rcmd" 0]}{:set ["j-rcmd" 0]}{:set ["k-rcmd" 0]}
                                {:set ["l-rcmd" 0]}{:set ["m-rcmd" 0]}{:set ["n-rcmd" 0]}{:set ["o-rcmd" 0]}
                                {:set ["p-rcmd" 0]}{:set ["q-rcmd" 0]}{:set ["r-rcmd" 0]}{:set ["s-rcmd" 0]}
                                {:set ["t-rcmd" 0]}{:set ["u-rcmd" 0]}{:set ["v-rcmd" 0]}{:set ["w-rcmd" 0]}
                                {:set ["x-rcmd" 0]}{:set ["y-rcmd" 0]}{:set ["z-rcmd" 0]}{:set ["openbracket-rcmd" 0]}
                                {:set ["closebracket-rcmd" 0]}{:set ["bslash-rcmd" 0]}{:set ["semi-rcmd" 0]}
                                {:set ["quote-rcmd" 0]}{:set ["return-rcmd" 0]}{:set ["caps-rcmd" 0]}{:set ["rshift-rcmd" 0]}
                                {:set ["lshift-rcmd" 0]}{:set ["space-rcmd" 0]}{:set ["ropt-rcmd" 0]}{:set ["lcmd-rcmd" 0]}
                        ]
                }
                
                :ropt {:key :right_option :condi [:!rcmd] 
                        :afterup [{:set ["ropt" 0]}]
                }
        }

; -- CUSTOM MODES & LAYERS --
        :main [
                {:des "Custom Modes/Layers Creation" :rules [
                        ; DEL MODE - I need it to work with modifiers and could not figure out how to do it as template. Works on F19 and caps lock. NOT on SD Mode.
                                [{:key :semicolon :modi {:optional [:caps_lock]}} ["del-mode" 1]
                                        [:!ropt :!rcmd :!w-mode :!spacebarpress :!caps-mode :!t-mode :!a-mode :!sd-mode :!f-mode :!e-mode]
                                        {:afterup ["del-mode" 0] :alone {:key :semicolon :repeat false}}]
                        ; W MODE: Window Management
                                [:w nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!f-mode :!e-mode :!bananotate]
                                        {:alone {:key :w :halt true} :delayed {:canceled {:key :w}}
                                        :held {:set ["w-mode" 1] :repeat false}
                                        :afterup {:set ["w-mode" 0]}}]   
                        ; E MODE: Numpad & Symbols
                                        [:#Pe nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!f-mode :!holdbana-mode]
                                        {:alone {:key :e :halt true} :delayed {:canceled {:key :e}}
                                        :held {:set ["e-mode" 1] :repeat false}
                                        :afterup {:set ["e-mode" 0]}
                                        :params {:held 120 :delay 120 :alone 119}}]
                        ; T MODE: Markdown and Text Transformations
                                [:#Pt nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!a-mode :!sd-mode :!w-mode :!f-mode :!e-mode]
                                        {:alone {:key :t :halt true} :delayed {:canceled {:key :t}}
                                        :held {:set ["t-mode" 1] :repeat false}
                                        :afterup {:set ["t-mode" 0]}}]
                        ; CAPS & F19 MODES: Quick tap CAPS = ESCAPE and CAPS MODE. Held = f19 Mode (closes CAPS MODE).
                                [:##caps_lock ["caps-mode" 1]
                                        [:!ropt :!rcmd :!del-mode :!f19-mode :!t-mode :!a-mode :!sd-mode :!w-mode :!f-mode :!e-mode :!bananotate]
                                        {:afterup [["caps-mode" 0] ["f19-mode" 0]] :alone {:key :escape}
                                        :held [["caps-mode" 0] ["f19-mode" 1]]      ; <- this could also press and hold a key with :held [["caps-mode" 0] ["f19-mode" 1] {:key :f19}]
                                        :params {:held 160 :delay 160 :alone 159}}]
                        ; HOLDBANA MODE: Quick tap CAPS = ESCAPE and CAPS MODE. Similar as above with F19 Mode
                                [:##caps_lock ["caps-mode" 1]
                                        [:bananotate :!caps-mode :!ropt :!rcmd :!del-mode :!f19-mode :!t-mode :!a-mode :!sd-mode :!w-mode :!f-mode :!e-mode]
                                        {:afterup [["caps-mode" 0] ["holdbana-mode" 0]] :alone {:key :escape}
                                        :held [["caps-mode" 0] ["holdbana-mode" 1]]
                                        :params {:held 160 :delay 160 :alone 159}}]
                        ; A MODE: KIKI AI
                                [:a nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!f-mode :!e-mode :!bananotate]
                                        {:alone {:key :a :halt true} :delayed {:canceled {:key :a}}
                                        :held {:set ["a-mode" 1] :repeat false}
                                        :afterup {:set ["a-mode" 0]}}]                      
                        ; SD MODE: Text, Window, and Tabs Navigation (Super Duper Mode)
                                [{:sim [:s :d] :simo {:interrupt true :dorder :insensitive :uorder :insensitive
                                        :afterup {:set ["sd-mode" 0]}} :modi {:optional :any}} {:set ["sd-mode" 1]}
                                        [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!a-mode :!w-mode :!f-mode :!e-mode]]
                        ; SW SuperWhisper Rec & Mode
                                [{:sim [:f :g] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}} [[:km "SW Goku Trigger"] {:set ["sw-mode" 1]} :!CTO0]
                                        [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode :!f-mode]
                                        {:afterup [[:km "SW Goku Trigger"] {:set ["sw-mode" 0]}]}]    
                        ; F MODE: Cursor Movement, Dragging and Clicking
                                        [:f nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]
                                                {:alone {:key :f :halt true} :delayed {:canceled {:key :f}}
                                                :held {:set ["f-mode" 1] :repeat false}
                                                :afterup {:set ["f-mode" 0]}}]
                ]},

                {:des "Misc Sublayers Creation" :rules [
                        ; wD-Press
                                [:d [["wD-press" 1]] [["w-mode" 1]["wC-press" 0]["wV-press" 0]["wR-press" 0]["wG-press" 0]["wF-press" 0]] {:afterup {:set ["wD-press" 0]}}]
