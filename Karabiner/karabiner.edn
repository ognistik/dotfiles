                                        [:!ropt :!rcmd :!w-mode :!wR-press :!wD-press :!wF-press :!wG-press :!wC-press :!wV-press :!e-mode :!t-mode :!caps-mode :!capsE-press :!capsA-press :!capsS-press :!capsD-press :!capsF-press :!capsC-press :!f19-mode :!a-mode :!sd-mode :!sw-mode :!f-mode :!del-mode :!holdBri-mode :brilliant]
                                        {:afterup [["caps-mode" 0]["holdBri-mode" 0]["hideOverlay" 0]] :alone {:key :escape}
                                        :held [["caps-mode" 0]["holdBri-mode" 1][:km "Karabiner Active Layer" "holdBri-mode,◬"]]
                                        :params {:held 160 :delay 160 :alone 159}}]
                        ; A MODE: KIKI AI
                                [:a nil [:!ropt :!rcmd :!w-mode :!wR-press :!wD-press :!wF-press :!wG-press :!wC-press :!wV-press :!e-mode :!t-mode :!caps-mode :!capsE-press :!capsA-press :!capsS-press :!capsD-press :!capsF-press :!capsC-press :!f19-mode :!a-mode :!sd-mode :!sw-mode :!f-mode :!del-mode :!holdBri-mode]
                                        {:alone {:key :a :halt true} :delayed {:canceled {:key :a}}
                                        :held [{:set ["a-mode" 1] :repeat false} [:km "Karabiner Active Layer" "a-mode,A"]]
                                        :afterup [["a-mode" 0]["hideOverlay" 0]]}]                      
                        ; SD MODE: Text, Window, and Tabs Navigation (Super Duper Mode)
                                [{:sim [:s :d] :simo {:interrupt true :dorder :insensitive :uorder :insensitive
                                        :afterup {:set ["sd-mode" 0]}} :modi {:optional :any}} {:set ["sd-mode" 1]}
                                        [:!ropt :!rcmd :!w-mode :!wR-press :!wD-press :!wF-press :!wG-press :!wC-press :!wV-press :!e-mode :!t-mode :!caps-mode :!capsE-press :!capsA-press :!capsS-press :!capsD-press :!capsF-press :!capsC-press :!f19-mode :!a-mode :!sd-mode :!sw-mode :!f-mode :!del-mode :!holdBri-mode]]
                        ; SW Superwhisper Rec & Mode
                                [{:sim [:d :f] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}} [[:btt "swResultOff"] {:set ["sw-mode" 1]} :!CTO0]
                                        [:!ropt :!rcmd :!w-mode :!wR-press :!wD-press :!wF-press :!wG-press :!wC-press :!wV-press :!e-mode :!t-mode :!caps-mode :!capsE-press :!capsA-press :!capsS-press :!capsD-press :!capsF-press :!capsC-press :!f19-mode :!a-mode :!sd-mode :!sw-mode :!f-mode :!del-mode :!holdBri-mode]
                                        {:afterup [[:km "Trigger - KeyPress"] {:set ["sw-mode" 0]}]}]
                        ; F MODE: Cursor Movement, Dragging and Clicking
                                        [:f nil [:!ropt :!rcmd :!w-mode :!wR-press :!wD-press :!wF-press :!wG-press :!wC-press :!wV-press :!e-mode :!t-mode :!caps-mode :!capsE-press :!capsA-press :!capsS-press :!capsD-press :!capsF-press :!capsC-press :!f19-mode :!a-mode :!sd-mode :!sw-mode :!f-mode :!del-mode :!holdBri-mode]
                                                {:alone {:key :f :halt true} :delayed {:canceled {:key :f}}
                                                :held {:set ["f-mode" 1] :repeat false}
                                                :afterup {:set ["f-mode" 0]}}]
                ]},

                {:des "Misc Sublayers Setup" :rules [
                        ;wR-Press
