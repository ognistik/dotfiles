;;-----------------------------------------LAYERS CREATION---------------------------------------------------------------------------------------------------
        :main [
       {:des "Modes/Layers Creation"
                :rules [
        ;;DEL MODE - I need it to work with modifiers and could not figure out how to do it as template. Works on F19. NOT on SD Mode.
                [:semicolon ["del-mode" 1]
                        [:!ropt :!rcmd :!w-mode :!spacebarpress :!caps-mode :!t-mode :!a-mode :!sd-mode :!f-mode :!e-mode]
                        {:afterup ["del-mode" 0] :alone {:key :semicolon :repeat false}}]
                [:!Psemicolon ["del-mode" 1]
                        [:!ropt :!rcmd :!w-mode :!spacebarpress :!caps-mode :!t-mode :!a-mode :!sd-mode :!f-mode :!e-mode]
                        {:afterup ["del-mode" 0] :alone {:key :semicolon :repeat false}}]
        ;;W MODE: Window Management
                [:w nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!f-mode :!e-mode]
                        {:alone {:key :w :halt true} :delayed {:canceled {:key :w}}
                        :held {:set ["w-mode" 1] :repeat false}
                        :afterup {:set ["w-mode" 0]}}]   
        ;;E MODE: Numpad & Symbols
                [:e nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!f-mode]
                        {:alone {:key :e :halt true} :delayed {:canceled {:key :e}}
                        :held {:set ["e-mode" 1] :repeat false}
                        :afterup {:set ["e-mode" 0]}}] 
        ;;T MODE: Markdown and Text Transformations
                [:t nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!a-mode :!sd-mode :!w-mode :!f-mode :!e-mode]
                        {:alone {:key :t :halt true} :delayed {:canceled {:key :t}}
                        :held {:set ["t-mode" 1] :repeat false}
                        :afterup {:set ["t-mode" 0]}}]
        ;;CAPS & F19 MODES: Quick tap CAPS = ESCAPE and CAPS MODE. Held = f19 for Superkey Seek (closes CAPS MODE).
                [:##caps_lock ["caps-mode" 1]
                        [:!ropt :!rcmd :!del-mode :!f19-mode :!t-mode :!a-mode :!sd-mode :!w-mode :!f-mode :!e-mode]
                        {:afterup [["caps-mode" 0] ["f19-mode" 0]] :alone {:key :escape}
                        :held [["caps-mode" 0] ["f19-mode" 1] {:key :f19}]
                        :params {:held 160 :delay 160 :alone 159}}]
        ;;A MODE: KIKI AI
                [:a nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!f-mode :!e-mode]
                        {:alone {:key :a :halt true} :delayed {:canceled {:key :a}}
                        :held {:set ["a-mode" 1] :repeat false}
                        :afterup {:set ["a-mode" 0]}}]                      
        ;;SD MODE: Text, Window, and Tabs Navigation (Super Duper Mode)
               [{:sim [:s :d] :simo {:interrupt true :dorder :insensitive :uorder :insensitive
                        :afterup {:set ["sd-mode" 0]}} :modi {:optional :any}} {:set ["sd-mode" 1]}
                        [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!a-mode :!w-mode :!f-mode :!e-mode]]
        ;;F MODE: Cursor Movement, Dragging and Clicking
                [:f nil [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]
                        {:alone {:key :f :halt true} :delayed {:canceled {:key :f}}
                        :held {:set ["f-mode" 1] :repeat false}
                        :afterup {:set ["f-mode" 0]}}] 
        ;;ITHILWEN SIMLAYER
                [{:sim [:d :f] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}}
                :!Tf16 [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]]
        ;;APHYRA SIMLAYER
                [{:sim [:s :f] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}}
                :!TOf16 [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]]
        ;;TEXENDUR
                [{:sim [:c :v] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}}
                :!COf16 [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]]
        ;;SELDARIEN
                [{:sim [:x :v] :simo {:interrupt true :dorder :insensitive :uorder :insensitive}}
                :!Of16 [:!ropt :!rcmd :!del-mode :!f19-mode :!caps-mode :!t-mode :!sd-mode :!w-mode :!a-mode :!e-mode]]
              ]
       },

