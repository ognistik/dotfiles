                        ;capsS-press
                                [:s [["capsS-press" 1][:km "Karabiner Active Layer" "capsS-press,▲ / S"]] [["caps-mode" 1]["capsA-press" 0]["capsE-press" 0]["capsS-press" 0]["capsD-press" 0]["capsF-press" 0]["capsC-press" 0]]]
