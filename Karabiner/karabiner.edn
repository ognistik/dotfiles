                        ; capsS-press
                                [:s [["capsS-press" 1]] [["caps-mode" 1]["capsD-press" 0]["capsE-press" 0]["capsC-press" 0]] {:afterup {:set ["capsS-press" 0]}}]
                        ; capsD-press
                                [:d [["capsD-press" 1]] [["caps-mode" 1]["capsS-press" 0]["capsE-press" 0]["capsC-press" 0]] {:afterup {:set ["capsD-press" 0]}}]
                        ; capsC-press
                                [:c [["capsC-press" 1]] [["caps-mode" 1]["capsS-press" 0]["capsE-press" 0]["capsD-press" 0]] {:afterup {:set ["capsC-press" 0]}}]
                        ; aD-press
                                [:d [["aD-press" 1]] [["a-mode" 1]["aW-press" 0]] {:afterup {:set ["aD-press" 0]}}]
                        ; aW-press
                                [:w [["aW-press" 1]] [["a-mode" 1]["aD-press" 0]] {:afterup {:set ["aW-press" 0]}}]
                        ; fSpace-press
                                [:##spacebar ["fSpace-press" 1] ["f-mode" 1] {:afterup {:set ["fSpace-press" 0]}}]
                ]},

