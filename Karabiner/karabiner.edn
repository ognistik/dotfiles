                        ; wV-Press
                                [:v [["wV-press" 1]] [["w-mode" 1]["wC-press" 0]["wD-press" 0]["wR-press" 0]["wG-press" 0]["wF-press" 0]] {:afterup {:set ["wV-press" 0]}}]
                        ; eF-Press
