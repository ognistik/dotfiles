; -- DELETE MODS (& :CONDI Sample) --
                {:des "Delete Mods" :rules [
                        ; Disable the backspace key...
                                [:delete_or_backspace :vk_none]
                        ; Enable caps lock left + right shift
                                [:!Sright_shift :caps_lock [:!ropt :!rcmd :!w-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!e-mode]]   
                        ; Disable caps lock left + right shift while on caps lock      
                                [:!PSright_shift :caps_lock [:!ropt :!rcmd :!w-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!e-mode]]    
                        ; Single tap right shift = delete forward    
                                [:##right_shift :right_shift [:!ropt :!rcmd :!w-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!e-mode]
                                        {:alone {:key :delete_forward}}]
                        ; Single tap left shift = delete backward
                                [:##left_shift :left_shift [:!ropt :!rcmd :!w-mode :!caps-mode :!t-mode :!a-mode :!sd-mode :!e-mode]
                                        {:alone {:key :delete_or_backspace}}]                                                                  
                        ; DEL MODE LAYER - This serves is an example of using :condi (affects all lines following)
                                ; Delete words backward while pressing semicolon + a | outside Terminal
                                        [:condi :del-mode :!terminal] [:##a :!Odelete_or_backspace] 
                                ; Delete words backward while pressing semicolon + a | in Terminal
                                        [:condi :del-mode :terminal] [:##a :!Tw]                        
                ]},

