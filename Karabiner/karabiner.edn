                                [:v [["wV-press" 1][:km "Karabiner Active Layer" "wC-press,W / V"]] [["w-mode" 1]["wR-press" 0]["wD-press" 0]["wF-press" 0]["wG-press" 0]["wC-press" 0]["wV-press" 0]]]
